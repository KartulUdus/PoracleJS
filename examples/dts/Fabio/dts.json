[
  {
    "id": "Normal",
    "type": "monster",
    "language": "en",
    "default": true,
    "platform": "discord",
    "template": {
      "embed": {
        "color": "{{ivcolor}}",
        "title": "{{#eq bestGreatLeagueRank 1}}ü•á {{else eq bestUltraLeagueRank 1}}ü•á {{else eq bestGreatLeagueRank 2}}ü•à {{else eq bestUltraLeagueRank 2}}ü•à {{else eq bestGreatLeagueRank 3}}ü•â {{else eq bestUltraLeagueRank 3}}ü•â {{/eq}}{{round iv}}% {{fullName}}{{#if disguisePokemonName}} (disguised as {{disguisePokemonName}}){{/if}}{{#eq id 29}}{{else eq id 32}}{{else}} {{genderData.emoji}}{{/eq}} L{{level}}{{#gte iv 100}}{{else}} ({{atk}}|{{def}}|{{sta}}){{/gte}}{{#if boostemoji}} {{boostemoji}}{{/if}} ‚è± {{time}}",
        "description": "{{zipcode}} {{city}}, {{addr}}{{> futureEvent_en}}",
        "thumbnail": {
          "url": "{{{imgUrlAlt}}}"
        },
        "image": {
          "url": "{{{staticMap}}}"
        },
        "fields": [
          {
            "name": "Despawn",
            "value": "{{time}} (<t:{{disappear_time}}:R>) {{#if weatherChange}}{{weatherChange}}{{/if}}{{#unless confirmedTime}}üÖæÔ∏è{{/unless}}",
            "inline": false
          },
          {
            "name": "Stats",
            "value": "{{round iv}}% IV ({{atk}}/{{def}}/{{sta}}) | {{cp}} CP (Lvl. {{level}})\nWeight: {{weight}}kg | Height: {{height}}m{{#if form}}{{#isnt formname 'Normal'}}\nForm: {{formname}}{{/isnt}}{{/if}}",
            "inline": false
          },
          {
            "name": "Moveset",
            "value": "{{quickMove}} {{{quickMoveEmoji}}} | {{chargeMove}} {{{chargeMoveEmoji}}}",
            "inline": false
          },
          {
            "name": "PVP",
            "value": "{{#lt (sum (sum pvpLittle.length pvpGreat.length) pvpUltra.length) 3}}{{> pvplcup_en}}{{> pvpgbl_en}}{{> pvpubl_en}}{{else}}{{> pvplcup_pu_en}}{{> pvpgbl_pu_en}}{{> pvpubl_pu_en}}{{/lt}}{{#isFalsey pvpAvailable}}N/A{{/isFalsey}}",
            "inline": false
          },
          {
            "name": "Links",
            "value": "[Map]({{{reactMapUrl}}}) | [Google](<S<{{{googleMapUrl}}}>S>) | [Apple](<S<{{{appleMapUrl}}}>S>)",
            "inline": false
          }
        ],
        "timestamp": "{{nowISO}}",
        "footer": {
          "text": "Area: {{areas}}"
        }
      }
    }
  },
